{
  "connection": "default",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "nome": {
      "default": "",
      "type": "string"
    },
    "foto": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "ativo": {
      "default": true,
      "type": "boolean"
    },
    "endereco": {
      "default": "",
      "type": "string"
    },
    "numero": {
      "default": "",
      "type": "integer"
    },
    "bairro": {
      "default": "",
      "type": "string"
    },
    "cidade": {
      "default": "",
      "type": "string"
    },
    "estado": {
      "default": "",
      "type": "string"
    },
    "cep": {
      "default": "",
      "type": "string"
    },
    "empresacons": {
      "collection": "empresacon",
      "via": "user"
    },
    "empresacres": {
      "collection": "empresacre",
      "via": "user"
    },
    "telefone": {
      "default": "",
      "type": "string"
    },
    "adminCredenciado": {
      "type": "boolean",
      "default": false,
      "configurable": true
    },
    "adminContratante": {
      "type": "boolean",
      "default": false,
      "configurable": true
    },
    "verify": {
      "default": false,
      "type": "boolean"
    },
    "vistorias": {
      "collection": "vistoria",
      "via": "user"
    }
  }
}
